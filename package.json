{
  "name": "reca",
  "version": "0.0.1",
  "description": "ReCA - React Clean Architecture state manager",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc && clean:dist-tests && npm run test:module",
    "clean": "rimraf ./dist",
    "clean:dist-tests": "rimraf ./dist/__fixtures__ && rimraf ./dist/__tests__",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:module": "node ./dist/index.js",
    "prepublish": "npm run test && npm run build"
  },
  "keywords": [
    "react",
    "state manager"
  ],
  "author": "LabEG",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@labeg/code-style": "^2.0.31",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-jest": "^27.5.1",
    "enzyme": "^3.11.0",
    "jest": "^27.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  }
}
