{
  "name": "reca",
  "version": "0.0.2",
  "description": "ReCA - React Clean Architecture state manager",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc && clean:dist-tests && npm run test:module",
    "clean": "rimraf ./dist",
    "cs:eslint": "eslint --fix -c .eslintrc.cjs --ext .tsx,.ts,.jsx,.js ./src/",
    "clean:dist-tests": "rimraf ./dist/__fixtures__ && rimraf ./dist/__tests__",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:module": "node ./dist/index.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "react",
    "state manager"
  ],
  "lint-staged": {
      "./src/**/*.(ts|tsx|js|jsx)": [
          "eslint --fix -c .eslintrc.js --ext .tsx,.ts,.jsx,.js"
      ]
  },
  "author": "LabEG",
  "license": "MIT",
  "dependencies": {
    "first-di": "^0.1.44"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@labeg/code-style": "^2.0.33",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.5.0",
    "@types/react": "^18.0.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-jest": "^28.1.0",
    "enzyme": "^3.11.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
